<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <message_queue>
            <mysqlmq>
                <retry_inprogress_after>1440</retry_inprogress_after>
                <new_messages_lifetime>10080</new_messages_lifetime>
                <successful_messages_lifetime>10080</successful_messages_lifetime>
                <failed_messages_lifetime>10080</failed_messages_lifetime>
            </mysqlmq>
        </message_queue>
    </default>
</config>
